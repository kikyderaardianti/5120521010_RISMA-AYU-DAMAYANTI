<?xml version="1.0" encoding="UTF-8" ?>
<widget id="com.risma.myapp" version="2.0.0.0" android-versionCode="1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
	<name>Mengenal Makanan Khas Daerah Jawa</name>
	<description>game edukasi makanan khas daerah jawa</description>
	<author href="http://rismaayu/" email="ayudamayanti1804@gmail.com">rismaayu</author>

	<content src="index.html"/>
	<icon src="icon.png" />

	<platform name="android">
		<allow-intent href="market:*"/>
		<allow-intent href="http://*/*"/>
		<allow-intent href="https://*/*"/>
		<allow-intent href="tel:*"/>
		<allow-intent href="sms:*"/>
		<allow-intent href="mailto:*"/>
		<allow-intent href="geo:*"/>

		<preference name="enabled" value="true"/>
		<preference name="deployment-target" value="8.0" />
		<preference name="android-minSdkVersion" value="19" />
		<preference name="android-targetSdkVersion" value="29" />
	

	</platform>	

	
	<feature name="http://api.phonegap.com/1.0/network"/>
	<feature name="http://api.phonegap.com/1.0/file" />
	<preference name="phonegap-version" value="cli-9.0.0" />
	<preference name="orientation" value="landscape" />
	<preference name="fullscreen" value="true" />

	<access origin="*" />
	<plugin name="cordova-plugin-whitelist" />
	<plugin name="cordova-plugin-file" />
	<plugin name="cordova-plugin-media" />

</widget>